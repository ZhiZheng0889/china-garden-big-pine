{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npx cross-env CI=true npm run test:run",
    "test:run": "run-s test:unit test:e2e",
    "test:0": "run-p -1 r test:start:* \"test:e2e:run -- us-00\"",
    "test:e2e": "run-p -l -r test:start:* test:e2e:run",
    "test:e2e:run": "wait-on http://localhost:5000/foods && jest --detectOpenHandles --forceExit --config ./e2e/jest.config.js",
    "test:start:backend": "npm run test:start --prefix ./../backend",
    "test:start:frontend": "npx cross-env BROWSER=none NODE_ENV=test react-scripts start",
    "test:unit": "react-scripts test"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "google-map-react": "^2.2.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "cross-fetch": "^3.1.5",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "puppeteer": "^19.7.4",
    "tailwindcss": "^3.2.4",
    "vite": "^4.1.1"
  }
}
